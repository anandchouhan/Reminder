<tr> 
	<th scope="row"><%= team.name %></th> 
	<td><%= team.admin.name %></td> 
	<td><%= link_to 'Edit', edit_team_path(team.id), class: "btn btn-info btn-sm glyphicon glyphicon-edit", "aria-hidden"=>"true", "data-toggle"=>"modal", "data-target"=>"#myModal" %>
	  <%= link_to 'Delete',  team_path(team.id), data: {confirm: "Do you want delete?"},class: "btn btn-danger btn-sm glyphicon glyphicon-trash", "aria-hidden" => "true", method: :delete %>
	  <%= link_to "Add members", new_team_team_member_path(team.id), class: "btn btn-success btn-sm glyphicon glyphicon-plus", "aria-hidden" => "true" %>   
	</td> 
</tr> 